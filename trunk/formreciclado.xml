<script>

function backCall(){

		if(permiso=="USA"){
		var aux = arguments[0].papel; 
		var aux1 = arguments[0].toner;
		var aux2 = arguments[0].rraaee;
		//console.log("aux "+ aux);
		   if(aux!=""){
				$('#papelkg').val(arguments[0].papel / 2.2);
				$('#papelkg').toString().replace(/\./,",");
		   }else{
				document.getElementById('papelkg').value = "";
		   
		   }
		   if(aux1!=""){
			 $('#tonerkg').val(arguments[0].toner / 2.2);
			 $('#tonerkg').toString().replace(/\./,",");
		   }else{
			document.getElementById('tonerkg').value = "";
		   }
		   
		   if(aux2!=""){
				$('#rraaees').val(arguments[0].rraaee / 2.2);
				$('#rraaees').toString().replace(/\./,",");
			}else{
				document.getElementById('rraaees').value = "";
			}
			console.log("arguments[0].numequipos "+arguments[0].numequipos);
			$('#numequipos').val(arguments[0].numequipos.toString().replace(/\./,","));
		
		}else if(permiso=="ESPAÑA CORPORATIVOS"){
			$('#papelkg').val(arguments[0].papel.toString().replace(/\./,","));
			$('#tonerkg').val(arguments[0].toner.toString().replace(/\./,","));
			$('#residuos').val(arguments[0].residuos.toString().replace(/\./,","));
			
		}else{
			$('#papelkg').val(arguments[0].papel.replace(/\./,","));
			if(permiso!="ESPAÑA"){
			$('#tonerkg').val(arguments[0].toner.replace(/\./,","));
			}
			$('#rraaees').val(arguments[0].rraaee.replace(/\./,","));
			
		    $('#numequipos').val(arguments[0].numequipos.replace(/\./,","));
			if(permiso="ADMIN"){
			$('#residuos').val(arguments[0].residuos.replace(/\./,","));
			}
		}
       
       
       
       
}

function getData(){
        var anio = $('#anyo').val();
        var meses = $('#meses').val();
        var country = $('#country').val();
		
		if((permiso != "ADMIN")&&(permiso !="ESPAÑA CORPORATIVOS")){ 
			country = permiso;
		}else if(permiso =="ESPAÑA CORPORATIVOS"){
			country = "CORPORATIVOS";
		
		}	
		//console.log("countryy "+country);
		//console.log("mesess "+meses);
        $.ajax({
                type: "POST",
                url: "https://script.google.com/macros/s/AKfycbyFMmwVCMIF1HuiAYCVNx6d5R2aaN5CNINGKp0wU1EGy8cMNBml/exec?callback=backCall",   //"https://script.google.com/macros/s/AKfycbyFMmwVCMIF1HuiAYCVNx6d5R2aaN5CNINGKp0wU1EGy8cMNBml/exec?callback=backCall",
                dataType: "jsonp",
                data: { anio: anio, meses: meses, country: country }
                }).done(function( msg ) {
                alert( "Data Saved: " + msg );
        });
       
}

